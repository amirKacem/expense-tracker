{
    "name": "app/expennies",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\":"tests/"
        }
    },
    "require": {
        "doctrine/dbal": "^3.7",
        "doctrine/migrations": "^3.7",
        "doctrine/orm": "^2.17",
        "php-di/php-di": "^7.0",
        "slim/csrf": "^1.4",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12",
        "slim/twig-view": "^3.3",
        "symfony/cache": "^6.4",
        "symfony/console": "^6.4",
        "symfony/twig-bridge": "^6.1",
        "symfony/webpack-encore-bundle": "^1.15",
        "twig/intl-extra": "^3.8",
        "vlucas/phpdotenv": "^5.6",
        "vlucas/valitron": "^1.4"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "symfony/var-dumper": "^6.4"
    },
    "authors": [
        {
            "name": "amirKacem",
            "email": "amir.kassem.ben.ammou@gmail.com"
        }
    ]
}
